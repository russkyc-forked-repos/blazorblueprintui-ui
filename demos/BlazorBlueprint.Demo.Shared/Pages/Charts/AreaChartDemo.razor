    private string _selectedRange = "90d";

    private readonly SelectOption<string>[] _rangeOptions =
    [
        new("90d", "Last 3 months"),
        new("30d", "Last 30 days"),
        new("7d", "Last 7 days")
    ];

    private List<ChartData> _filteredChartData = [];

    private string _subtitleText => _selectedRange switch
    {
        "7d" => "last 7 days",
        "30d" => "last 30 days",
        _ => "last 3 months"
    };

    private string _dateRangeText => _selectedRange switch
    {
        "7d" => "June 24 - June 30, 2024",
        "30d" => "June 1 - June 30, 2024",
        _ => "April 1 - June 30, 2024"
    };

    protected override void OnInitialized()
    {
        ApplyFilter();
    }

    private void OnRangeChanged(string? value)
    {
        _selectedRange = value ?? "90d";
        ApplyFilter();
    }

    private void ApplyFilter()
    {
        var endDate = DateOnly.Parse("2024-06-30");
        var startDate = _selectedRange switch
        {
            "7d" => endDate.AddDays(-6),
            "30d" => endDate.AddDays(-29),
            _ => DateOnly.Parse("2024-04-01")
        };

        _filteredChartData = _chartData
            .Where(d => DateOnly.Parse(d.Date) >= startDate && DateOnly.Parse(d.Date) <= endDate)
            .ToList();
    }

