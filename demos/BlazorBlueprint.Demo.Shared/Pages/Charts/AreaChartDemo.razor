@page "/charts/area"
@using BlazorBlueprint.Icons.Lucide.Components
<PageTitle>Area Chart - Blazor Blueprint</PageTitle>

<div class="space-y-8 max-w-6xl">
    <div>
        <div class="space-y-3">
            <h1 class="text-4xl font-bold tracking-tight">Area Chart</h1>
            <p class="text-xl text-muted-foreground">
                Visualize data with filled areas that emphasize volume and magnitude over time.
            </p>
        </div>
    </div>

    <!-- Interactive Area Chart (full width hero) -->
    <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
        <div class="flex items-center justify-between p-6">
            <div class="flex flex-col space-y-1.5">
                <h3 class="font-semibold leading-none tracking-tight">Area Chart - Interactive</h3>
                <p class="text-sm text-muted-foreground">Showing total visitors for the @_subtitleText</p>
            </div>
            <BbSelect TValue="string" Value="@_selectedRange" ValueChanged="OnRangeChanged"
                    Options="@_rangeOptions" Class="w-[160px]" />
        </div>
        <div class="p-6 pt-0">
            <BbAreaChart Data="@_filteredChartData" Height="300px">
                <BbXAxis DataKey="date" />
                <BbYAxis />
                <BbChartTooltip />
                <BbChartLegend />
                <BbArea DataKey="desktop" Name="Desktop" Color="var(--chart-1)" Curve="CurveType.Smooth" Stacked="true">
                    <BbFill>
                        <BbLinearGradient>
                            <BbGradientStop Offset="0" Color="var(--chart-1)" Opacity="0.6" />
                            <BbGradientStop Offset="1" Color="var(--chart-1)" Opacity="0.05" />
                        </BbLinearGradient>
                    </BbFill>
                </BbArea>
                <BbArea DataKey="mobile" Name="Mobile" Color="var(--chart-2)" Curve="CurveType.Smooth" Stacked="true">
                    <BbFill>
                        <BbLinearGradient>
                            <BbGradientStop Offset="0" Color="var(--chart-2)" Opacity="0.6" />
                            <BbGradientStop Offset="1" Color="var(--chart-2)" Opacity="0.05" />
                        </BbLinearGradient>
                    </BbFill>
                </BbArea>
            </BbAreaChart>
        </div>
        <div class="flex flex-col gap-2 p-6 pt-0">
            <div class="flex gap-2 items-center font-medium leading-none">
                Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
            </div>
            <div class="text-sm text-muted-foreground leading-none">
                @_dateRangeText
            </div>
        </div>
    </div>
    <CodeBlock Source="Charts/AreaChart/interactive.txt" />

    <!-- Grid of chart variants -->
    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">

        <!-- Default (Smooth) -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Area Chart</h3>
                    <p class="text-sm text-muted-foreground">Showing total visitors for the last 6 months</p>
                </div>
                <div class="p-6 pt-0">
                    <BbAreaChart Data="@_monthlyData" Height="200px">
                        <BbXAxis DataKey="month" />
                        <BbYAxis Show="false" />
                        <BbChartTooltip />
                        <BbArea DataKey="desktop" Name="Desktop" Color="var(--chart-1)" Curve="CurveType.Smooth">
                            <BbFill>
                                <BbLinearGradient>
                                    <BbGradientStop Offset="0" Color="var(--chart-1)" Opacity="0.4" />
                                    <BbGradientStop Offset="1" Color="var(--chart-1)" Opacity="0.05" />
                                </BbLinearGradient>
                            </BbFill>
                        </BbArea>
                    </BbAreaChart>
                </div>
                <div class="flex flex-col gap-2 p-6 pt-0">
                    <div class="flex gap-2 items-center font-medium leading-none">
                        Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
                    </div>
                    <div class="text-sm text-muted-foreground leading-none">
                        January - June 2024
                    </div>
                </div>
            </div>
            <CodeBlock Source="Charts/AreaChart/default.txt" />
        </div>

        <!-- Linear -->
