@page "/charts/bar"
@using BlazorBlueprint.Icons.Lucide.Components
<PageTitle>Bar Chart - Blazor Blueprint</PageTitle>

<div class="space-y-8 max-w-6xl">
    <div>
        <div class="space-y-3">
            <h1 class="text-4xl font-bold tracking-tight">Bar Chart</h1>
            <p class="text-xl text-muted-foreground">
                Display data with vertical or horizontal bars for comparing values across categories.
            </p>
        </div>
    </div>

    <!-- Interactive Bar Chart (full width hero) -->
    <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
        <div class="flex items-start justify-between p-6">
            <div class="flex flex-col space-y-1.5">
                <h3 class="font-semibold leading-none tracking-tight">Bar Chart - Interactive</h3>
                <p class="text-sm text-muted-foreground">Showing total visitors for the last 3 months</p>
            </div>
            <div class="flex">
                <div class="flex flex-col gap-1 border-l px-6 py-0.5">
                    <span class="text-xs text-muted-foreground">Desktop</span>
                    <span class="text-2xl font-bold leading-none">@_chartData.Sum(d => d.Desktop).ToString("N0")</span>
                </div>
                <div class="flex flex-col gap-1 border-l px-6 py-0.5">
                    <span class="text-xs text-muted-foreground">Mobile</span>
                    <span class="text-2xl font-bold leading-none">@_chartData.Sum(d => d.Mobile).ToString("N0")</span>
                </div>
            </div>
        </div>
        <div class="p-6 pt-0">
            <BbBarChart Data="@_chartData" Height="300px">
                <BbXAxis DataKey="date" />
                <BbYAxis />
                <BbChartTooltip />
                <BbBar DataKey="desktop" Name="Desktop" Color="var(--chart-2)" />
            </BbBarChart>
        </div>
    </div>
    <CodeBlock Source="Charts/BarChart/interactive.txt" />

    <!-- Grid of chart variants -->
    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">

        <!-- Default -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Bar Chart</h3>
                    <p class="text-sm text-muted-foreground">January - June 2024</p>
                </div>
                <div class="p-6 pt-0">
                    <BbBarChart Data="@_monthlyData" Height="200px">
                        <BbXAxis DataKey="month" />
                        <BbYAxis Show="false" />
                        <BbChartTooltip />
                        <BbBar DataKey="desktop" Name="Desktop" Color="var(--chart-1)" />
                    </BbBarChart>
                </div>
                <div class="flex flex-col gap-2 p-6 pt-0">
                    <div class="flex gap-2 items-center font-medium leading-none">
                        Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
                    </div>
                    <div class="text-sm text-muted-foreground leading-none">
                        Showing total visitors for the last 6 months
                    </div>
                </div>
            </div>
            <CodeBlock Source="Charts/BarChart/default.txt" />
        </div>

        <!-- Horizontal -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Bar Chart - Horizontal</h3>
                    <p class="text-sm text-muted-foreground">January - June 2024</p>
                </div>
                <div class="p-6 pt-0">
                    <BbBarChart Data="@_monthlyData" Horizontal="true" Height="200px">
                        <BbXAxis DataKey="month" />
                        <BbYAxis Show="false" />
                        <BbChartTooltip />
                        <BbBar DataKey="desktop" Name="Desktop" Color="var(--chart-1)" />
                    </BbBarChart>
                </div>
                <div class="flex flex-col gap-2 p-6 pt-0">
                    <div class="flex gap-2 items-center font-medium leading-none">
                        Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
                    </div>
                    <div class="text-sm text-muted-foreground leading-none">
                        Showing total visitors for the last 6 months
                    </div>
                </div>
            </div>
            <CodeBlock Source="Charts/BarChart/horizontal.txt" />
        </div>

        <!-- Multiple -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Bar Chart - Multiple</h3>
                    <p class="text-sm text-muted-foreground">January - June 2024</p>
                </div>
                <div class="p-6 pt-0">
                    <BbBarChart Data="@_monthlyData" Height="200px">
                        <BbXAxis DataKey="month" />
                        <BbYAxis Show="false" />
                        <BbChartTooltip />
                        <BbBar DataKey="desktop" Name="Desktop" Color="var(--chart-1)" />
                        <BbBar DataKey="mobile" Name="Mobile" Color="var(--chart-2)" />
                    </BbBarChart>
                </div>
                <div class="flex flex-col gap-2 p-6 pt-0">
                    <div class="flex gap-2 items-center font-medium leading-none">
                        Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
                    </div>
                    <div class="text-sm text-muted-foreground leading-none">
                        Showing total visitors for the last 6 months
                    </div>
                </div>
            </div>
            <CodeBlock Source="Charts/BarChart/multiple.txt" />
        </div>

        <!-- Stacked + Legend -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Bar Chart - Stacked + Legend</h3>
                    <p class="text-sm text-muted-foreground">January - June 2024</p>
                </div>
                <div class="p-6 pt-0">
                    <BbBarChart Data="@_monthlyData" Height="200px">
                        <BbXAxis DataKey="month" />
                        <BbYAxis Show="false" />
                        <BbChartTooltip />
                        <BbChartLegend />
                        <BbBar DataKey="desktop" Name="Desktop" Color="var(--chart-1)" Stacked="true" />
                        <BbBar DataKey="mobile" Name="Mobile" Color="var(--chart-2)" Stacked="true" BorderRadius="4" />
                    </BbBarChart>
                </div>
                <div class="flex flex-col gap-2 p-6 pt-0">
                    <div class="flex gap-2 items-center font-medium leading-none">
                        Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
                    </div>
                    <div class="text-sm text-muted-foreground leading-none">
                        Showing total visitors for the last 6 months
                    </div>
                </div>
            </div>
            <CodeBlock Source="Charts/BarChart/stacked-legend.txt" />
        </div>

        <!-- Label -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Bar Chart - Label</h3>
                    <p class="text-sm text-muted-foreground">January - June 2024</p>
                </div>
                <div class="p-6 pt-0">
                    <BbBarChart Data="@_monthlyData" Height="200px">
                        <BbXAxis DataKey="month" />
                        <BbYAxis Show="false" />
                        <BbChartTooltip />
                        <BbBar DataKey="desktop" Name="Desktop" Color="var(--chart-1)" ShowLabel="true" />
                    </BbBarChart>
                </div>
                <div class="flex flex-col gap-2 p-6 pt-0">
                    <div class="flex gap-2 items-center font-medium leading-none">
                        Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
                    </div>
                    <div class="text-sm text-muted-foreground leading-none">
                        Showing total visitors for the last 6 months
                    </div>
                </div>
            </div>
            <CodeBlock Source="Charts/BarChart/label.txt" />
        </div>

        <!-- Custom Label -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Bar Chart - Custom Label</h3>
                    <p class="text-sm text-muted-foreground">January - June 2024</p>
                </div>
                <div class="p-6 pt-0">
                    <BbBarChart Data="@_labelData" Horizontal="true" Height="200px">
                        <BbXAxis DataKey="month" LabelInside="true" LabelColor="white" />
                        <BbYAxis Show="false" />
                        <BbChartTooltip />
                        <BbBar DataKey="desktop" Name="Desktop" Color="var(--chart-2)"
                             ShowLabel="true" LabelPosition="LabelPosition.Right" BorderRadius="8" />
                    </BbBarChart>
                </div>
                <div class="flex flex-col gap-2 p-6 pt-0">
                    <div class="flex gap-2 items-center font-medium leading-none">
                        Trending up by 5.2% this month <LucideIcon Name="trending-up" Size="16" />
                    </div>
                    <div class="text-sm text-muted-foreground leading-none">
                        Showing total visitors for the last 6 months
                    </div>
                </div>
            </div>
            <CodeBlock Source="Charts/BarChart/custom-label.txt" />
        </div>

        <!-- Mixed -->
        <div>
            <div class="rounded-xl border bg-card text-card-foreground shadow-sm">
                <div class="flex flex-col space-y-1.5 p-6">
                    <h3 class="font-semibold leading-none tracking-tight">Bar Chart - Mixed</h3>
                    <p class="text-sm text-muted-foreground">January - June 2024</p>
                </div>
                <div class="p-6 pt-0">
                    <BbBarChart Data="@_browserData" Horizontal="true" Height="200px">
