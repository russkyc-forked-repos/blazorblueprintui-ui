@namespace BlazorBlueprint.Components.Timeline

<ol aria-label="Timeline" class="@CssClass" @attributes="AdditionalAttributes">
    <CascadingValue Value="this" IsFixed="true">
        @if (ChildContent != null)
        {
            @ChildContent
        }
        else
        {
            <TimelineEmpty />
        }
    </CascadingValue>
</ol>
